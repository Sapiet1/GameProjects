[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64     = "res://../platformer/target/debug/libplatformer.so"
linux.release.x86_64   = "res://../platformer/target/release/libplatformer.so"
windows.debug.x86_64   = "res://../platformer/target/debug/platformer.dll"
windows.release.x86_64 = "res://../platformer/target/release/platformer.dll"
macos.debug            = "res://../platformer/target/debug/libplatformer.dylib"
macos.release          = "res://../platformer/target/release/libplatformer.dylib"
macos.debug.arm64      = "res://../platformer/target/debug/libplatformer.dylib"
macos.release.arm64    = "res://../platformer/target/release/libplatformer.dylib"
web.debug.wasm32       = "res://../platformer/target/wasm32-unknown-emscripten/debug/platformer.wasm"
web.release.wasm32     = "res://../platformer/target/wasm32-unknown-emscripten/release/platformer.wasm"
